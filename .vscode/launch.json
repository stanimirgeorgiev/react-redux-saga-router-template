{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "chrome",
      "request": "attach",
      "port": 9222,
      "name": "Launch Chrome debugging",
      "showAsyncStacks": true,
      "sourceMapPathOverrides": {
        "webpack:///./node_modules/*": "${workspaceFolder}/node_modules/*",
        "webpack:///./src/*": "${workspaceFolder}/src/*"
      },
      "webRoot": "${workspaceFolder}"
    }
  ]
}
